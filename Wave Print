#include <iostream>
using namespace std;

void wavePrint(int input[][1000], int row, int col){
	/* Don't write main().
	* Don't read input, it is passed as function argument.
	* Print output and don't return it.
	* Taking input is handled automatically.
	*/

    
for(int j =0;j<col;j++){
	
if(j%2==0){
	
for(int i=0;i<row;i++){
	
cout<<input[i][j]<<" ";	
	
	
	
	
}	
}
else	
	
	
{
for(int l=(row-1);l>=0;l--){
	
	
cout<<input[l][j]<<" ";	
	
	
}	
	
	
}	
	
	
	
	
	
	
}	
	
	
	
	
	
	
	    
    
    
    
    
    
    
    
    
    
    
}

